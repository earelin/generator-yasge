{
  "name": "generator-yasge",
  "version": "1.0.0-beta6",
  "description": "Yet another Spring Yeoman generator",
  "keywords": [
    "yeoman-generator",
    "Java",
    "Spring",
    "Spring Boot",
    "Spring Security",
    "JPA",
    "Hibernate",
    "Docker",
    "Gradle"
  ],
  "homepage": "https://github.com/earelin/generator-yasge",
  "bugs": "https://github.com/earelin/generator-yasge/issues",
  "author": {
    "name": "Xavier Carriba",
    "url": "https://github.com/earelin"
  },
  "scripts": {
    "test": "jest",
    "test:integration": "gulp test-integration",
    "test:all": "gulp test-all",
    "lint": "eslint commons generators"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/earelin/generator-yasge.git"
  },
  "files": [
    "app",
    "create-class",
    "create-controller",
    "create-repository",
    "create-service",
    "import-model"
  ],
  "main": "generators/app/index.js",
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.2",
    "gulp-beautify": "^2.0.1",
    "gulp-if": "^2.0.2",
    "gulp-pretty-data": "^0.1.2",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "object-to-xml": "^2.0.0",
    "request-promise-native": "^1.0.7",
    "yeoman-generator": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "gulp": "^4.0.0",
    "jest": "^24.0.0",
    "jest-junit": "^6.3.0",
    "yo": "^2.0.5"
  },
  "license": "GPL-3.0",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./reports/coverage",
    "collectCoverageFrom": [
      "commons/**/*.js",
      "generators/**/*.js",
      "!**/node_modules/**"
    ],
    "roots": [
      "<rootDir>/generatos/",
      "<rootDir>/commons/",
      "<rootDir>/tests/"
    ],
    "testRegex": [
      ".*\\.test\\.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./reports/test"
  }
}
